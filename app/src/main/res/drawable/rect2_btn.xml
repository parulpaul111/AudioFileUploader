<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#FF5722" />
            <gradient android:angle="-90" android:startColor="#FF9800" android:endColor="#FF9800"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#FF9800" />
            <solid android:color="#958C74"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#FF9800" />
            <gradient android:angle="-90" android:startColor="#E8E6D6" android:endColor="#E67F5E" />
        </shape>
    </item>
</selector>